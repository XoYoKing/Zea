source 'https://github.com/CocoaPods/Specs.git'

inhibit_all_warnings!
platform :ios, '10.0'

def seed_pods
    
    #网络请求
    pod 'AFNetworking'
    
    #SQLite
    pod 'FMDB/SQLCipher'
    
    #键盘
    pod 'IQKeyboardManager'
    
    #自动布局
    pod 'Masonry'
    
    #定制化的等待提示器
    pod 'MBProgressHUD'
    
    #上拉加载下拉刷新
    pod 'MJRefresh'
    
    #网络图像缓存
    pod 'SDWebImage'
    
    #图片选择
    #pod 'TZImagePickerController'
    
    #模型转换
    pod 'YYModel'
    
end

target 'Seed' do
    seed_pods
end

target 'SeedTests' do
    seed_pods
end

target 'SeedUITests' do
    seed_pods
end
